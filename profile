alias dj="python3 manage.py"
alias dj_mig="python3 manage.py makemigrations"
alias dj_mig_push="python3 manage.py migrate"
alias dj_run="python3 manage.py runserver"
alias dj_su="python3 manage.py createsuperuser"
alias dj_dump="python3 manage.py dumpdata"
alias dj_load="python3 manage.py loaddata data.json"
alias dj_shell="python manage.py shell"
alias dj_app="python3 manage.py startapp"
alias dj_cron="python3 manage.py runcrons"
alias dj_celery="celery -A config worker --loglevel=info"
alias dj_beat="celery -A config beat --loglevel=info"